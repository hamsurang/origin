{
  "name": "origin",
  "private": true,
  "scripts": {
    "clean:branch": "git fetch --prune && git branch | grep -v '^*' | xargs git branch -D",
    "build:home": "pnpm --filter home run build",
    "build:activity": "pnpm --filter activity run build",
    "build:web": "pnpm --filter web run build",
    "dev:web": "pnpm run dev:home & pnpm run dev:activity & pnpm --filter web run dev",
    "dev:home": "pnpm --filter home run dev",
    "dev:activity": "pnpm --filter activity run dev",
    "lint:home": "pnpm --filter home run lint",
    "lint:activity": "pnpm --filter activity run lint",
    "lint:web": "pnpm --filter web run lint",
    "format": "turbo format",
    "prepare": "husky",
    "preinstall": "corepack enable"
  },
  "devDependencies": {
    "@biomejs/biome": "1.7.3",
    "@hamsurang/typescript-config": "workspace:*",
    "husky": "^9.0.11",
    "nano-staged": "^0.8.0",
    "turbo": "^1.13.0",
    "typescript": "5.5.0-beta"
  },
  "packageManager": "pnpm@9.1.0",
  "engines": {
    "node": "20.13.0"
  }
}
